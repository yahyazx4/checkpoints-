def calculator(num1, num2):
    operator=input("enter_your_operator(+,-,*,/):")
    if operator == '+':
        return num1+num2
    elif operator == '-':
        return num1-num2
    elif operator == '*':
      return num1*+num2
    elif operator == '/':
        return num2 !=0
    else:
        return ("invalid syntax")
result = calculator(10,5)
print(result)